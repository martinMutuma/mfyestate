<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>


	<package name="stage" namespace="/" extends="global">
		<action name="*"
			class="com.estate.web.stage.action.StageAction"
			method="{1}"
		>
			<result >/hbody.jsp</result>
			<result name="bus">/home.jsp</result>
			<result name="proShow">${proShowPage}</result>
			<result name="needShow">/needShow.jsp</result>
			<result name="seniorSearch">/seniorSearchNew.jsp</result>
			<result name="seniorSearchMul">/seniorSearchMul.jsp</result>
			<result name="bulid">/bulidShow.jsp</result>
			<result name="bulidSearch">/bulidSearch.jsp</result>
			<result name="bulidKeyWord">/bulidKeyWord.jsp</result>
			<result name="bulidPrice">/bulidPrice.jsp</result>
			<result name="business">/businessShow.jsp</result>
			<result name="secondBulid">/secondBulid.jsp</result>
			<result name="rentHouse">/rentHouse.jsp</result>
			<result name="home" type="redirect">/home.shtml</result>
			<result name="tuan" >/tuanShow.jsp</result>
			<result name="special" >/specialsShow.jsp</result>
			<result name="jointuan" >/jointuan.jsp</result>
			<result name="sso" >/sso.jsp</result>
			<result name="annoumentList">/annoument.jsp</result>
			<result name="announcementShow">/anouncement_show.jsp</result>
		</action>
	</package>
	
	<package name="district" namespace="/district" extends="global">
		<action name="*"
			class="com.estate.web.stage.action.DistrictAction"
			method="{1}"
		>
		</action>
	</package>
</struts>
