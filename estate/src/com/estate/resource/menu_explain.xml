<?xml version="1.0" encoding="UTF-8"?>
<menuList>
	<menu>
		<name></name>
		<!--菜单名称-->
		<tip></tip>
		<!--提示信息-->
		<url></url>
		<!--菜单对应的路径-->
		<descr>1</descr>
		<!--菜单位置：主菜单从1开始一次排序-->
		<allow-groups enable="false"></allow-groups>
		<!-- 允许访问的用户组，多个用户组之间用,号隔开,属性值enable为false时表示该菜单有子菜单，如子菜单数量为0，则该菜单不显示，属性为true时，表明该菜单需要相应用户组才能进行访问 -->
		<children>
		<!-- 子菜单集合 -->
			<menu>
				<!-- 子菜单 -->
				<name>写字楼</name>
				<!-- 子菜单名称 -->
				<tip></tip>
				<!-- 子菜单提示信息 -->
				<url></url>
				<!-- 子菜单路径 -->
				<category-list>
				<!-- 子菜单类别集合 -->
					<category>
					<!-- 类别 -->
						<name>出售管理</name>
						<!-- 类别名称 -->
						<url></url>
						<!-- 类别路径 -->
					</category>
				</category-list>
				<descr>11</descr>
				<!--子菜单位置：主菜单位置+排序(从1开始)-->
				<allow-groups enable="true">0</allow-groups>
			</menu>
		</children>
	</menu>
</menuList>