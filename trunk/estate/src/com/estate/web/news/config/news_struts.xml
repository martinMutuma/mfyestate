<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="news" namespace="/news" extends="global">
		<action name="news_*"
			class="com.estate.web.news.action.newsAction"
			method="{1}"
		>
		<result name="toNews">/news/news.jsp</result>
		<result name="toNewsType">/news/newsType.jsp</result>
		<result name="toNewsInfo">/news/newsInfo.jsp</result>
		</action>
	</package>
</struts>
